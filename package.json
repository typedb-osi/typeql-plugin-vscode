{
    "name": "typeql",
    "displayName": "TypeQL",
    "description": "TypeQL Syntax Highlighter",
    "version": "0.4.3",
    "icon": "logo.png",
    "publisher": "typedb",
    "author": {
        "name": "TypeDB Community",
        "email": "community@typedb.com"
    },
    "contributors": [
        "Paul Bergmann",
        "Yash Bhagwat"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/typedb-osi/typeql-plugin-vscode"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.81.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "tql",
        "typeql",
        "typedb"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tql",
                "aliases": [
                    "TypeQL",
                    "typeql",
                    "tql"
                ],
                "extensions": [
                    ".tql"
                ],
                "configuration": "./language-configuration.json",
                "icon": { "light": "./logo.png", "dark": "./logo.png" }
            }
        ],
        "grammars": [
            {
                "language": "tql",
                "scopeName": "source.tql",
                "path": "./syntaxes/tql.tmLanguage.json"
            },
            {
                "path": "./syntaxes/tql-markdown-injection.json",
                "scopeName": "markdown.typeql.codeblock",
                "injectTo": ["text.html.markdown"],
                "embeddedLanguages": {
                    "meta.embedded.block.typeql": "tql"
                }
            }
        ],
        "markdown.preview.breaks": true,
        "markdown.preview.syntaxHighlighting": true,
        
        "configurationDefaults": {
            "editor.insertSpaces": false,
            "editor.tokenColorCustomizations": {
                "textMateRules": [
                    {
                        "scope": "keyword.operator.words.tql",
                        "settings": {
                            "foreground": "#ff7abd"
                        }
                    },
                    {
                        "scope": "keyword.operator.characters.tql",
                        "settings": {
                            "foreground": "#ff7abd"
                        }
                    },
                    {
                        "scope": "storage.type.tql",
                        "settings": {
                            "foreground": "#55EAE2",
                            "fontStyle": "italic"
                        }
                    },
                    {
                        "scope": "storage.modifier.tql",
                        "settings": {
                            "foreground": "#82b6ff",
                            "fontStyle": "italic"
                        }
                    },
                    {
                        "scope": "keyword.control.tql",
                        "settings": {
                            "foreground": "#ff7abd"
                        }
                    },
                    {
                        "scope": "keyword.annotations.tql",
                        "settings": {
                            "foreground": "#FFA187"
                        }
                    },
                    {
                        "scope": "support.function.tql",
                        "settings": {
                            "foreground": "#ff7abd",
                            "fontStyle": "italic"
                        }
                    },
                    {
                        "scope": "string.tql",
                        "settings": {
                            "foreground": "#FFE4A7"
                        }
                    },
                    {
                        "scope": "constant.character.escape.tql",
                        "settings": {
                            "foreground": "#FFA187"
                        }
                    },
                    {
                        "scope": "constant.language.tql",
                        "settings": {
                            "foreground": "#FFA187"
                        }
                    },
                    {
                        "scope": "constant.numeric.tql",
                        "settings": {
                            "foreground": "#82b6ff"
                        }
                    },
                    {
                        "scope": "variable.parameter.tql",
                        "settings": {
                            "foreground": "#55EAE2",
                            "fontStyle": "italic"
                        }
                    },
                    {
                        "scope": "invalid.illegal.tql",
                        "settings": {
                            "foreground": "#ff8080"
                        }
                    }
                ],
                "[*Light*]": {
                    "textMateRules": [
                        {
                            "scope": "keyword.operator.words.tql",
                            "settings": {
                                "foreground": "#d4006a"
                            }
                        },
                        {
                            "scope": "keyword.operator.characters.tql",
                            "settings": {
                                "foreground": "#d4006a"
                            }
                        },
                        {
                            "scope": "storage.type.tql",
                            "settings": {
                                "foreground": "#0e8a84",
                                "fontStyle": "italic"
                            }
                        },
                        {
                            "scope": "storage.modifier.tql",
                            "settings": {
                                "foreground": "#0055c4",
                                "fontStyle": "italic"
                            }
                        },
                        {
                            "scope": "keyword.control.tql",
                            "settings": {
                                "foreground": "#d4006a"
                            }
                        },
                        {
                            "scope": "keyword.annotations.tql",
                            "settings": {
                                "foreground": "#b35232"
                            }
                        },
                        {
                            "scope": "support.function.tql",
                            "settings": {
                                "foreground": "#d4006a",
                                "fontStyle": "italic"
                            }
                        },
                        {
                            "scope": "string.tql",
                            "settings": {
                                "foreground": "#c48500"
                            }
                        },
                        {
                            "scope": "constant.character.escape.tql",
                            "settings": {
                                "foreground": "#b35232"
                            }
                        },
                        {
                            "scope": "constant.language.tql",
                            "settings": {
                                "foreground": "#b35232"
                            }
                        },
                        {
                            "scope": "constant.numeric.tql",
                            "settings": {
                                "foreground": "#0055c4"
                            }
                        },
                        {
                            "scope": "variable.parameter.tql",
                            "settings": {
                                "foreground": "#0e8a84",
                                "fontStyle": "italic"
                            }
                        },
                        {
                            "scope": "invalid.illegal.tql",
                            "settings": {
                                "foreground": "#cc0000"
                            }
                        }
                    ]
                }
            }
        }
    }
}
