{
    "name": "typeql",
    "displayName": "TypeQL Syntax",
    "description": "TypeQL Syntax Highlighter",
    "version": "0.4.1",
    "icon": "logo.png",
    "publisher": "typedb-osi",
    "author": {
        "name": "TypeDB Community",
        "email": "community@typedb.com"
    },
    "contributors": [
        "Paul Bergmann",
        "Yash Bhagwat"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/typedb-osi/typeql-plugin-vscode"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.81.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "tql",
        "typeql",
        "typedb"
    ],
    "contributes": {
        "languages": [{
          "id": "tql",
          "aliases": ["TypeQL", "tql"],
          "extensions": [".tql"],
          "configuration": "./language-configuration.json"
        }],
        "grammars": [{
          "language": "tql",
          "scopeName": "source.tql",
          "path": "./syntaxes/tql.tmLanguage.json"
        }],
        "configurationDefaults": {
            "editor.insertSpaces": false,
            "editor.tokenColorCustomizations": {
                "textMateRules": [
                    {
                        "scope": "comments.tql",
                        "settings": {
                            "foreground": "#4dc97c",
                            "fontStyle": "italic"
                        }
                    },

                    {
                        "scope": "keyword.operator.words.tql",
                        "settings": {
                            "foreground": "#ff7abd"
                        }
                    },
            
                    {
                        "scope": "keyword.operator.characters.tql",
                        "settings": {
                            "foreground": "#ff7abd"
                        }
                    },
            
                    {
                        "scope": "storage.type.tql",
                        "settings": {
                            "foreground": "#55EAE2",
                            "fontStyle": "italic"
                        }
                    },
            
                    {
                        "scope": "storage.modifier.tql",
                        "settings": {
                            "foreground": "#82b6ff",
                            "fontStyle": "italic"
                        }
                    },
            
                    {
                        "scope": "keyword.control.tql",
                        "settings": {
                            "foreground": "#ff7abd"
                        }
                    },
            
                    {
                        "scope": "keyword.annotations.tql",
                        "settings": {
                            "foreground": "#FFA187"
                        }
                    },
            
                    {
                        "scope": "support.function.tql",
                        "settings": {
                            "foreground": "#ff7abd",
                            "fontStyle": "italic"
                        }
                    },
            
                    {
                        "scope": "string.tql",
                        "settings": {
                            "foreground": "#FFE4A7"
                        }
                    },
            
                    {
                        "scope": "constant.character.escape.tql",
                        "settings": {
                            "foreground": "#FFA187"
                        }
                    },
            
                    {
                        "scope": "constant.language.tql",
                        "settings": {
                            "foreground": "#FFA187"
                        }
                    },
            
                    {
                        "scope": "constant.numeric.tql",
                        "settings": {
                            "foreground": "#82b6ff"
                        }
                    },
            
                    {
                        "scope": "variable.parameter.tql",
                        "settings": {
                            "foreground": "#55EAE2",
                            "fontStyle": "italic"
                        }
                    },
            
                    {
                        "scope": "invalid.illegal.tql",
                        "settings": {
                            "foreground": "#ff8080"
                        }
                    }
                ]
            }
        }
    }
}
